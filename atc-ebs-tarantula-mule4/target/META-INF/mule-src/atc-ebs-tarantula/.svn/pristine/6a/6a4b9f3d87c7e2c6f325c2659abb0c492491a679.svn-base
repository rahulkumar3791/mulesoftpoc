    <munit:test name="businessLogic-test-suite-tarantula-InsertOrdersFlowTest-InSertDataSuccess" description="Test">
    	<set-variable variableName="requestTime" value="#[server.dateTime]" doc:name="Set Request Time"/>
        <expression-component doc:name="Expression"><![CDATA[import java.util.ArrayList;
import java.util.List;

import com.atc.model.tarantula.RequisitionMoveOrder;
import com.atc.model.tarantula.RequisitionMoveOrderList;
RequisitionMoveOrder req1 = new RequisitionMoveOrder();
		req1.setItemCode("21");
		req1.setNeedbyDate("06-SEP-15");
		req1.setGlobalSiteID("21");
		req1.setCustomerName("21");
		req1.setSupplierName("21");
		req1.setQuantity(21);
		req1.setUnitPrice(21);
		req1.setUOMCode("21");
		req1.setServicePurpose("21");
		req1.setServiceDesc("21");
		req1.setRequestor("21");
		req1.setRequestDate("06-SEP-15");
		req1.setBoqLineID(21);
		req1.setBoqID("21");

		List Req = new ArrayList();
		Req.add(req1);

		RequisitionMoveOrderList param = new RequisitionMoveOrderList();
		param.setReq(Req);
		payload = param;
]]></expression-component>
        <flow-ref name="tarantula-InsertOrdersFlow" doc:name="Flow-ref to tarantula-InsertOrdersFlow"/>
        <munit:assert-on-equals expectedValue="an audit log record" actualValue="#[payload.description]" doc:name="Assert Equals"/>
        <munit:assert-on-equals expectedValue="atc-esb-tarantula-inbound" actualValue="#[payload.service]" doc:name="Assert Equals"/>
        <munit:assert-on-equals expectedValue="SUCCESS" actualValue="#[payload.status.message]" doc:name="Assert Equals"/>
    </munit:test>
    

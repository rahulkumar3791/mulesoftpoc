    <munit:test name="new-test-suite-ebs-getLandLeaseDataFlowTest-HavaResponse" description="Test">
     	<set-variable variableName="requestTime" value="#[server.dateTime]" doc:name="Set Request Time"/>
        <expression-component doc:name="Expression"><![CDATA[payload = '520001';]]></expression-component>
        <flow-ref name="ebs-getLandLeaseDataFlow" doc:name="Flow-ref to ebs-getLandLeaseDataFlow"/>
        <munit:assert-on-equals expectedValue="SUCCESS" actualValue="#[payload.header.statusCode]" doc:name="Assert Equals"/>
        <munit:assert-on-equals expectedValue="SUCCESS" actualValue="#[payload.header.statusDescription]" doc:name="Assert Equals"/>
    </munit:test>
